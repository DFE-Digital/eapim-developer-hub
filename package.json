    "compression": "^1.7.1",
    "cookie": "^0.4.1",
    "cookie-parser": "^1.4.5",
    "crypto": "^1.0.1",
    "express": "^4.16.2",
    "govuk-frontend": "^3.6.0",
    "helmet": "^3.20.0",
    "humps": "^2.0.1",
    "immutable": "^3.8.2",
    "isomorphic-unfetch": "^3.0.0",
    "jest-dom": "^4.0.0",
    "js-cookie": "^2.2.1",
    "jwt-decode": "^2.2.0",
    "moment": "^2.26.0",
    "moment-timezone": "^0.5.31",
    "msal": "^1.3.1",
    "next": "^9.3.6",
    "next-redux-wrapper": "^5.0.0",
    "next-routes": "^1.4.2",
    "node-sass": "^4.14.1",
    "nookies": "^2.3.0",
    "pify": "^5.0.0",
    "prop-types": "^15.6.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "react-html-parser": "^2.0.2",
    "react-is": "^16.13.1",
    "react-redux": "^7.0.3",
    "react-testing-library": "^8.0.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.1.1",
    "styled-normalize": "^8.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^25.1.0",
    "babel-plugin-inline-dotenv": "^1.1.2",
    "babel-plugin-module-resolver": "^4.0.0",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.8",
    "nodemon": "^2.0.1",
    "pre-commit": "^1.2.2",
    "react-test-renderer": "^16.2.0",
    "rimraf": "^3.0.0",
    "snazzy": "^8.0.0",
    "standard": "^13.0.1"
  },
  "pre-commit": "lint-staged",
  "lint-staged": {
    "*.js": [
      "yarn lint:fix"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "env": [
      "jest"
    ]
  },
  "engines": {
    "node": ">= 10"
  },
  "jest": {
    "modulePaths": [
      "./src",
      "./src/test"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ]
  },
  "author": "Craig Smith",
  "description": "Next js and react and express and redux"
}
